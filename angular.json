{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-nepali-datepicker": {
      "projectType": "library",
      "root": "projects/angular-nepali-datepicker",
      "sourceRoot": "projects/angular-nepali-datepicker/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/angular-nepali-datepicker/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-nepali-datepicker/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "angular-nepali-datepicker-ve": {
      "projectType": "library",
      "root": "projects/angular-nepali-datepicker-ve",
      "sourceRoot": "projects/angular-nepali-datepicker-ve/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/angular-nepali-datepicker-ve/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-nepali-datepicker-ve/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "defaultProject": "angular-nepali-datepicker"
}
